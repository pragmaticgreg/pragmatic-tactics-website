{
  "designSystem": {
    "name": "Strategic Planning Framework",
  "version": "1.0.0",
    "description": "A comprehensive design system extracted from strategic planning framework interface",
  
  "colors": {
    "primary": {
        "background": "#1a1a1a",
        "backgroundSecondary": "#2a2a2a",
        "backgroundTertiary": "#333333"
    },
    "semantic": {
        "success": "#10b981",
        "successDark": "#047857",
        "successLight": "#34d399",
        "warning": "#f59e0b",
        "warningDark": "#d97706",
        "error": "#ef4444",
        "errorDark": "#dc2626",
        "info": "#8b5cf6",
        "infoDark": "#7c3aed"
      },
      "text": {
        "primary": "#ffffff",
        "secondary": "#d1d5db",
        "tertiary": "#9ca3af",
        "muted": "#6b7280"
      },
      "border": {
        "primary": "#404040",
        "secondary": "#525252",
        "accent": "#6b7280"
      },
      "accent": {
        "purple": "#8b5cf6",
        "purpleDark": "#7c3aed",
        "teal": "#14b8a6",
        "tealDark": "#0f766e"
    }
  },

  "typography": {
      "fontFamilies": {
        "primary": "system-ui, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, 'Helvetica Neue', Arial, sans-serif",
        "secondary": "Inter, system-ui, sans-serif"
    },
    "fontSizes": {
        "xs": "0.75rem",
        "sm": "0.875rem",
        "base": "1rem",
        "lg": "1.125rem",
        "xl": "1.25rem",
        "2xl": "1.5rem",
        "3xl": "1.875rem",
        "4xl": "2.25rem"
    },
    "fontWeights": {
      "normal": 400,
      "medium": 500,
      "semibold": 600,
        "bold": 700
    },
    "lineHeights": {
      "tight": 1.25,
      "normal": 1.5,
      "relaxed": 1.625,
      "loose": 2
    },
    "letterSpacing": {
      "tight": "-0.025em",
        "normal": "0",
        "wide": "0.025em"
    }
  },

  "spacing": {
    "scale": {
        "xs": "0.25rem",
        "sm": "0.5rem",
        "md": "1rem",
        "lg": "1.5rem",
        "xl": "2rem",
        "2xl": "3rem",
        "3xl": "4rem",
        "4xl": "6rem",
        "5xl": "8rem"
      },
      "component": {
        "cardPadding": "1.5rem",
        "cardGap": "2rem",
        "sectionGap": "3rem",
        "iconSize": "1.5rem",
        "iconGap": "1rem"
    }
  },

  "borderRadius": {
      "sm": "0.25rem",
      "md": "0.5rem",
      "lg": "0.75rem",
      "xl": "1rem",
      "2xl": "1.5rem",
    "full": "9999px"
  },

  "shadows": {
      "sm": "0 1px 2px 0 rgba(0, 0, 0, 0.05)",
    "md": "0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -1px rgba(0, 0, 0, 0.06)",
    "lg": "0 10px 15px -3px rgba(0, 0, 0, 0.1), 0 4px 6px -2px rgba(0, 0, 0, 0.05)",
    "xl": "0 20px 25px -5px rgba(0, 0, 0, 0.1), 0 10px 10px -5px rgba(0, 0, 0, 0.04)",
      "inner": "inset 0 2px 4px 0 rgba(0, 0, 0, 0.06)",
      "glow": "0 0 20px rgba(139, 92, 246, 0.3)"
  },

  "components": {
      "card": {
        "base": {
          "backgroundColor": "colors.primary.backgroundSecondary",
          "borderRadius": "borderRadius.lg",
          "border": "1px solid colors.border.primary",
          "padding": "spacing.component.cardPadding",
          "transition": "all 0.2s ease-in-out"
        },
        "variants": {
          "default": {
            "backgroundColor": "colors.primary.backgroundSecondary",
            "borderColor": "colors.border.primary"
          },
      "primary": {
            "backgroundColor": "colors.primary.backgroundSecondary",
            "borderColor": "colors.accent.purple",
            "boxShadow": "0 0 0 1px colors.accent.purple"
          },
          "success": {
            "backgroundColor": "colors.semantic.successDark",
            "borderColor": "colors.semantic.success",
            "boxShadow": "0 0 0 1px colors.semantic.success"
          },
          "warning": {
            "backgroundColor": "colors.primary.backgroundSecondary",
            "borderColor": "colors.semantic.warning",
            "boxShadow": "0 0 0 1px colors.semantic.warning"
          }
        }
      },

      "icon": {
        "base": {
          "width": "spacing.component.iconSize",
          "height": "spacing.component.iconSize",
          "display": "flex",
          "alignItems": "center",
          "justifyContent": "center",
          "borderRadius": "borderRadius.full"
        },
        "variants": {
          "success": {
            "backgroundColor": "colors.semantic.success",
            "color": "colors.text.primary"
          },
          "warning": {
            "backgroundColor": "colors.semantic.warning",
            "color": "colors.text.primary"
          },
          "error": {
            "backgroundColor": "colors.semantic.error",
            "color": "colors.text.primary"
          },
          "info": {
            "backgroundColor": "colors.accent.purple",
            "color": "colors.text.primary"
          }
        }
      },

      "text": {
        "title": {
          "fontSize": "typography.fontSizes.xl",
          "fontWeight": "typography.fontWeights.semibold",
          "color": "colors.text.primary",
          "lineHeight": "typography.lineHeights.tight",
          "marginBottom": "spacing.md"
        },
        "subtitle": {
          "fontSize": "typography.fontSizes.lg",
          "fontWeight": "typography.fontWeights.medium",
          "color": "colors.text.primary",
          "lineHeight": "typography.lineHeights.normal",
          "marginBottom": "spacing.sm"
        },
        "body": {
          "fontSize": "typography.fontSizes.base",
          "fontWeight": "typography.fontWeights.normal",
          "color": "colors.text.secondary",
          "lineHeight": "typography.lineHeights.relaxed"
        },
        "caption": {
          "fontSize": "typography.fontSizes.sm",
          "fontWeight": "typography.fontWeights.normal",
          "color": "colors.text.tertiary",
          "lineHeight": "typography.lineHeights.normal"
        }
      },

      "connectionLine": {
        "base": {
          "stroke": "colors.border.accent",
          "strokeWidth": "1",
          "strokeDasharray": "4 4",
          "fill": "none",
          "opacity": "0.6"
        }
      },

      "layout": {
        "grid": {
          "display": "grid",
          "gap": "spacing.component.cardGap",
          "variants": {
            "threeColumn": {
              "gridTemplateColumns": "repeat(3, 1fr)"
            },
            "singleColumn": {
              "gridTemplateColumns": "1fr"
            }
          }
        },
        "container": {
          "maxWidth": "1200px",
          "margin": "0 auto",
          "padding": "spacing.xl"
        }
      }
    },

    "patterns": {
      "strategicFramework": {
        "description": "A visual framework showing principle → problems → solutions → outcome flow",
        "structure": {
          "centralPrinciple": {
            "component": "card",
            "variant": "primary",
            "icon": "lightning",
            "iconVariant": "info",
            "position": "top-center"
          },
          "problemCards": {
            "component": "card",
            "variant": "warning",
            "icon": "alert",
            "iconVariant": "warning",
            "layout": "threeColumn",
            "position": "middle-row"
          },
          "solutionCards": {
            "component": "card",
            "variant": "default",
            "icon": "checkmark",
            "iconVariant": "success",
            "layout": "threeColumn",
            "position": "lower-row"
          },
          "outcome": {
            "component": "card",
            "variant": "success",
            "icon": "star",
            "iconVariant": "success",
            "position": "bottom-center"
          }
        },
        "connections": {
          "principleToProblems": "connectionLine",
          "problemsToSolutions": "connectionLine",
          "solutionsToOutcome": "connectionLine"
        }
      },

      "cardWithIcon": {
        "description": "Standard card pattern with icon, title, and description",
        "structure": {
          "icon": {
            "component": "icon",
            "position": "top-left"
          },
          "title": {
            "component": "text",
            "variant": "subtitle"
          },
          "description": {
            "component": "text",
            "variant": "body"
          }
        }
      }
    },

    "breakpoints": {
      "mobile": "640px",
      "tablet": "768px",
      "desktop": "1024px",
      "wide": "1280px"
  },

  "animations": {
    "transitions": {
        "fast": "0.1s ease-in-out",
        "normal": "0.2s ease-in-out",
      "slow": "0.3s ease-in-out"
    },
      "easings": {
        "ease": "ease",
        "easeIn": "ease-in",
        "easeOut": "ease-out",
        "easeInOut": "ease-in-out"
    }
  },

  "accessibility": {
    "focusRing": {
        "width": "2px",
        "style": "solid",
        "color": "colors.accent.purple",
        "offset": "2px"
    },
    "contrast": {
      "minimum": "4.5:1",
      "enhanced": "7:1"
      }
    }
  }
} 